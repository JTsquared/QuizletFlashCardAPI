{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "MockBrowserUserAgent": "Mozilla/5.0 (Macintosh; Intel Mac OS X 10.14; rv:68.0) Gecko/20100101 Firefox/68.0",

  "FlashCardDOMQueries": {
    "BaseUrl": "https://quizlet.com/subject/",
    "QuizLinkQuery": "div.SearchPage-result.js-setResult header.SetPreview-cardHeader a",
    "FlashCardElementsQuery": "div.SetPageTerms-term",
    "FlashCardQuestionQuery": "a.SetPageTerm-wordText span.TermText.notranslate.lang-en",
    "FlashCardAnswerQuery": "a.SetPageTerm-definitionText span.TermText.notranslate.lang-en"
  }
}
